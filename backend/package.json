    "@types/node": "^25.0.10",
    "prisma": "^5.22.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@prisma/client": "^5.22.0",
    "@types/jsonwebtoken": "^9.0.8",
    "bcrypt": "^5.1.1",
    "fastify": "^4.26.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4"
  }
